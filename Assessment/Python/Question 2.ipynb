    "def dict(n):\n",
    "    d = { 'Square': lambda a : a**2, \n",
    "         'Cube': lambda a : a**3, \n",
    "         'Squareroot': lambda a : a**(1/2) }\n",
    "    res= 0\n",
    "    for key in d.keys():\n",
    "        res+= d[key](n)\n",
    "    return res\n",
    "    \n",
    "print(dict(5))"
   ]
